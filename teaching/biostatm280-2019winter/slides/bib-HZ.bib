	Language = {English},
	Number = {416},
	Pages = {899-909},
	Publisher = {American Statistical Association},
	Title = {Using {EM} to Obtain Asymptotic Variance-Covariance Matrices: the {SEM} Algorithm},
	Url = {http://www.jstor.org/stable/2290503},
	Volume = {86},
	Year = {1991},
