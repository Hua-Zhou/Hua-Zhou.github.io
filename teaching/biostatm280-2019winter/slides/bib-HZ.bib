	Number = {7145},
	Pages = {661--678},
	Posted-At = {2008-06-16 21:31:41},
	Priority = {2},
	Publisher = {Nature Publishing Group},
	Title = {Genome-wide association study of 14,000 cases of seven common diseases and 3,000 shared controls},
	Url = {http://dx.doi.org/10.1038/nature05911},
	Volume = {447},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nature05911}}

