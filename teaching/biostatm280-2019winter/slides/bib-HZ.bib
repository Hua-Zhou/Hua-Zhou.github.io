	Journal = {Bioinformatics},
	Number = {12},
	Pages = {i204-i212},
	Title = {A multivariate regression approach to association analysis of a quantitative trait network},
	Url = {http://bioinformatics.oxfordjournals.org/content/25/12/i204.abstract},
	Volume = {25},
	Year = {2009},
	Bdsk-Url-1 = {http://bioinformatics.oxfordjournals.org/content/25/12/i204.abstract},
